status = input('–í–≤–µ–¥–∏—Ç–µ –Ω–∞–∑–≤–∞–Ω–∏–µ —Å—Ç–∞—Ç—É—Å–∞ –≤–∞—à–µ–≥–æ –∑–∞–∫–∞–∑–∞(–≤ –æ–∂–∏–¥–∞–Ω–∏–∏, –≤ –æ–±—Ä–∞–±–æ—Ç–∫–µ, –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ, –¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æ, –æ—Ç–º–µ–Ω–µ–Ω–æ): ')

match status:
    case '–≤ –æ–∂–∏–¥–∞–Ω–∏–∏':
        condition = '–≤ –æ–∂–∏–¥–∞–Ω–∏–∏'
        description = '–ó–∞–∫–∞–∑ —Ç–æ–ª—å–∫–æ —Å–æ–∑–¥–∞–Ω –∏ —Å–µ–π—á–∞—Å –Ω–∞—Ö–æ–¥–∏—Ç—Å—è –Ω–∞ —Å–±–æ—Ä–∫–µ'
        time = '–í –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç –≤—Ä–µ–º—è —Ä–∞—Å—á–∏—Ç–∞—Ç—å –Ω–µ–≤–æ–∑–º–æ–∂–Ω–æ'
        recomendations = '–ü—Ä–æ–≤–µ—Ä—è–π—Ç–µ —Å—Ç–∞—Ç—É—Å –∑–∞–∫–∞–∑–∞!'
        emoji = 'üïê'

    case '–≤ –æ–±—Ä–∞–±–æ—Ç–∫–µ':
        condition = '–í –æ–±—Ä–∞–±–æ—Ç–∫–µ'
        description = '–ó–∞–∫–∞–∑ –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–µ—Ç—Å—è —Å–ª—É–∂–±–æ–π –¥–æ—Å—Ç–∞–≤–∫–∏'
        time = '3-8 –¥–Ω–µ–π'
        recomendations = '–û–∂–∏–¥–∞–π—Ç–µ —Å–∫–æ—Ä–æ–π –¥–æ—Å—Ç–∞–≤–∫–∏ –∑–∞–∫–∞–∑–∞!'
        emoji = 'üì¶'

    case '–æ—Ç–ø—Ä–∞–≤–ª–µ–Ω–æ':
        condition = '–ó–∞–∫–∞–∑ –æ—Ç–ø—Ä–∞–≤–ª–µ–Ω'
        description = '–í–∞—à –∑–∞–∫–∞–∑ —É–∂–µ –µ–¥–µ—Ç –∫ –≤–∞–º'
        time = '1-5 –¥–Ω–µ–π'
        recomendations = '–°–ª–µ–¥–∏—Ç–µ –∑–∞ —É–≤–µ–¥–æ–º–ª–µ–Ω–∏–µ–º –æ –¥–æ—Å—Ç–∞–≤–∫–µ –∑–∞–∫–∞–∑–∞ –Ω–∞ –ø—É–Ω–∫—Ç –≤—ã–¥–∞—á–∏'
        emoji = '‚úàÔ∏è'

    case '–¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æ':
        condition = '–ó–∞–∫–∞–∑ –¥–æ—Å—Ç–∞–≤–ª–µ–Ω'
        description = '–í–∞—â –∑–∞–∫–∞–∑ –æ–∂–∏–¥–∞–µ—Ç –≤ –ø—É–Ω–∫—Ç–µ –≤—ã–¥–∞—á–∏ –∑–∞–∫–∞–∑–æ–≤'
        time = '–ì–æ—Ç–æ–≤ –∫ –≤—ã–¥–∞—á–µ'
        recomendations = '–ó–∞–±–µ—Ä–∏—Ç–µ –∑–∞–∫–∞–∑ –≤ —Ç–µ—á–µ–Ω–∏–∏ 7-–º–∏ –¥–Ω–µ–π —Å –º–æ–º–µ–Ω—Ç–∞ –¥–æ—Å—Ç–∞–≤–∫–∏'
        emoji = '‚úÖ'

    case '–æ—Ç–º–µ–Ω–µ–Ω–æ':
        condition = '–û—Ç–º–µ–Ω–µ–Ω–æ'
        description = '–ó–∞–∫–∞–∑ –±—ã–ª –æ—Ç–º–µ–Ω–µ–Ω'
        time = '-'
        recomendations = '–í–∞—à –∑–∞–∫–∞–∑ –±—ã–ª –æ—Ç–º–µ–Ω–µ–Ω, –¥–µ–Ω—å–≥–∏ –±—ã–ª–∏ –≤–æ–∑–≤—Ä–∞—â–µ–Ω—ã –Ω–∞ —Å—á–µ—Ç'
        emoji = '‚ùå'

    case _:
        condition = '–û—à–∏–±–∫–∞'
        description = '–¢–∞–∫–æ–≥–æ —Å—Ç–∞—Ç—É—Å–∞ –∑–∞–∫–∞–∑–∞ –≤ –Ω–∞—à–µ–π —Å–∏—Å—Ç–µ–º–µ –Ω–µ —Å—É—â–µ—Å—Ç–≤—É–µ—Ç,–≤–≤–µ–¥–∏—Ç–µ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π –≤–∞—Ä–∏–∞–Ω'
        time = '-'
        recomendations = '–í–≤–µ–¥–∏—Ç–µ –ø—Ä–∞–≤–∏–ª—å–Ω—ã–π —Å—Ç–∞—Ç—É—Å –∑–∞–∫–∞–∑–∞'
        emoji = 'üö©'

print('‚ïî‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïó')
print('|    –°—Ç–∞—Ç—É—Å –≤–∞—à–µ–≥–æ –∑–∞–∫–∞–∑–∞üì¶   |')
print('‚ïö‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïê‚ïù')
print(f'–°—Ç–∞—Ç—É—Å: {condition} {emoji}')
print(f'–û–ø–∏—Å–∞–Ω–∏–µ: {description}')
print(f'–ü—Ä–∏–º–µ—Ä–Ω–æ–µ –≤—Ä–µ–º—è –¥–æ—Å—Ç–∞–≤–∫–∏: {time}')
print(f'–†–µ–∫–æ–º–µ–Ω–¥–∞—Ü–∏–∏: {recomendations}')


